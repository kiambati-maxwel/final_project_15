<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <data noupdate="1">
       <record id="ir_chicken_cull_sequence" model="ir.sequence">
           <field name="name">Cull Reference</field>
           <field name="code">cull.reference</field>
           <field name="prefix">CULL/%(day)s/%(month)s/%(y)s/</field>
           <field name="padding">4</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

<!--       ====== Batches ==============-->
         <record id="ir_chicken_batch_sequence" model="ir.sequence">
           <field name="name">Chicken Batch Reference</field>
           <field name="code">batch.sequence</field>
           <field name="prefix">BATCH/%(month)s/%(y)s/</field>
           <field name="padding">3</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

<!--       ======= Eggs Entries -->
        <record id="ir_chicken_eggs_sequence" model="ir.sequence">
           <field name="name">Eggs Entries Sequence</field>
           <field name="code">eggs.entries.sequence</field>
           <field name="prefix">EGGS/%(day)s/%(month)s/%(y)s/</field>
           <field name="padding">4</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>
       <!--       ======= feeding Entries -->
        <record id="ir_chicken_feeding_sequence" model="ir.sequence">
           <field name="name">Feeding Entries Sequence</field>
           <field name="code">feeding.sequence</field>
           <field name="prefix">FEED/%(day)s/%(month)s/%(y)s/</field>
           <field name="padding">4</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

        <record id="ir_chicken_vaccination_sequence" model="ir.sequence">
           <field name="name">Vaccination Entries Sequence</field>
           <field name="code">vaccination.sequence</field>
           <field name="prefix">VAC/%(day)s/%(month)s/%(y)s/</field>
           <field name="padding">4</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>



   </data>
</odoo>